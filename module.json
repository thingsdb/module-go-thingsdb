{
    "doc": "https://github.com/thingsdb/module-go-thingsdb#readme",
    "version": "0.1.0",
    "main": {
        "linux/386": "bin/thingsdb_linux_386.bin",
        "linux/amd64": "bin/thingsdb_linux_amd64.bin",
        "linux/arm": "bin/thingsdb_linux_arm.bin",
        "linux/arm64": "bin/thingsdb_linux_arm64.bin",
        "freebsd/386": "bin/thingsdb_freebsd_386.bin",
        "freebsd/amd64": "bin/thingsdb_freebsd_amd64.bin",
        "freebsd/arm": "bin/thingsdb_freebsd_arm.bin",
        "freebsd/arm64": "bin/thingsdb_freebsd_arm64.bin",
        "darwin/amd64": "bin/thingsdb_darwin_amd64.bin"
    },
    "exposes": {
        "query": {
            "doc": "Perform a query",
            "argmap": ["scope", "code", "vars"],
            "defaults": {
                "load": false,
                "deep": 1
            }
        },
        "run": {
            "doc": "Run a procedure",
            "argmap": ["scope", "name", "args"],
            "defaults": {
                "load": false,
                "deep": 1
            }
        }
    }
}